<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>False</Beta>
  <Category>MediaServer:Video</Category>
  <Name>youtube-dl-webui</Name>
  <Description>
    Another web front end for youtube-dl.[br][br]
    [b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]
    [b]/youtube-dl[/b] This is where youtube-dl will download all videos to.[br][br]
  </Description>
  <Overview>
    Another web front end for youtube-dl.[br][br]
  </Overview>
  <Support>https://github.com/RyanTheAllmighty/unraid-docker-templates/issues</Support>
  <Registry>https://registry.hub.docker.com/u/d0u9/youtube-dl-webui</Registry>
  <GitHub>https://github.com/d0u9/docker/tree/master/dockerfiles/youtube-dl-webui</GitHub>
  <Repository>d0u9/youtube-dl-webui</Repository>
  <Project>https://github.com/d0u9/youtube-dl-webui</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>PORT</Name>
      <Value>5000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/config/youtube-dl-webui</HostDir>
      <ContainerDir>/etc</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5000]/</WebUI>
  <Icon>https://raw.githubusercontent.com/RyanTheAllmighty/unraid-docker-templates/master/ryantheallmighty/icons/youtube-dl-server.png</Icon>
  <ExtraParams></ExtraParams>
</Containers>
